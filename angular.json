{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "puntuaciones": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/puntuaciones",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/manifest.webmanifest",
              {
                "glob": "**/*",
                "input": "src/favicons",
                "output": "/favicons/"
              }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all",
              "serviceWorker": "ngsw-config.json"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "puntuaciones:build:production"
            },
            "development": {
              "buildTarget": "puntuaciones:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "icons": {
          "builder": "@jaimemartinmartin15/jei-devkit-angular-builders:generate-icons",
          "options": {
            "configFile": "src/app/svg/svg.json"
          }
        },
        "e2e": {
          "builder": "@jaimemartinmartin15/jei-devkit-angular-builders:e2e-puppeteer",
          "options": {
            "devServerTarget": "puntuaciones:serve",
            "testRunner": "jasmine",
            "pixelThreshold": 0,
            "imageThreshold": 0,
            "originalImagesFolder": "e2e/screenshots/originals/",
            "e2eResultImagesFolder": "e2e/screenshots/e2e-results/"
          },
          "configurations": {
            "production": {
              "devServerTarget": "puntuaciones:serve:production"
            }
          }
        }
      }
    }
  }
}